@model List<EPD_Finder.Models.ArticleResult>

<form asp-action="DownloadExcel" method="post">
    @foreach (var result in Model)
    {
        <input type="hidden" name="results" value="@Html.Raw(System.Text.Json.JsonSerializer.Serialize(result))" />
    }
    <button type="submit">Spara som Excel-fil</button>
</form>

<table class="table">
    <thead>
        <tr>
            <th>ArtNr</th>
            <th>EPD-länkar</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var result in Model)
        {
            <tr>
                <td>@result.ENumber</td>
                <td>@result.EpdLink</td>
            </tr>
        }
    </tbody>
</table>
